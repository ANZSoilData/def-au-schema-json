		"pH": {
			"$anchor": "pH",
			"@id": "nil:template",
			"title": "pH",
			"description": "pH.",
			"$comment": "Note different units for method scm:20K1 (Quick field approximations of ASS presence and associated lime requirement for drain-spoil). Experimenting with schema conditionals to manage this: https://json-schema.org/understanding-json-schema/reference/conditionals.html#if-then-else .",
			"type": "object",
			"range@type": [
				"sosa:ObservableProperty",
				"qudt:QuantityValue"
			],
			"required": [
				"hasResult",
				"usedProcedure"
			],
			"properties": {
				"hasResult": {
					"type": "object",
					"required": [
						"value",
						"unit"
					],
					"properties": {
						"value": {
							"$ref": "./qudt.json#value"
						},
						"unit": {
							"$ref": "./qudt.json#unit",
							"enum": [
								"unit:UNITLESS",
								"unit:MOL-PER-M3"
							]
						}
					}
				},
				"usedProcedure": {
					"enum": [
						"scm:14C1",
						"scm:20K1",
						"scm:4A1",
						"scm:4A2",
						"scm:4A3",
						"scm:4B1",
						"scm:4B2",
						"scm:4B3",
						"scm:4B4",
						"scm:4B5",
						"scm:4C1",
						"scm:4C2",
						"scm:4C3",
						"scm:4C4",
						"scm:4D1",
						"scm:4E1",
						"scm:4G1"
					]
				},
				"resultTime": {
					"$ref": "./sosa.json#resultTime"
				},
				"qualityOfObservation": {
					"$ref": "./sosa.json#qualityOfObservation"
				}
			},
			"if": {
				"properties": {
					"usedProcedure": {
						"const": "scm:20K1"
					}
				}
			},
			"then": {
				"properties": {
					"hasResult": {
						"properties": {
							"unit": {
								"const": "unit:MOL-PER-M3"
							}
						}
					}
				}
			},
			"else": {
				"properties": {
					"hasResult": {
						"properties": {
							"unit": {
								"const": "unit:UNITLESS"
							}
						}
					}
				}
			}
