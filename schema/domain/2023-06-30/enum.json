					"description": "Light clay"
				},
				{
					"const": "sp:field-texture-LMC",
					"description": "Light medium clay"
				},
				{
					"const": "sp:field-texture-LS",
					"description": "Loamy sand"
				},
				{
					"const": "sp:field-texture-MC",
					"description": "Medium clay"
				},
				{
					"const": "sp:field-texture-MHC",
					"description": "Medium heavy clay"
				},
				{
					"const": "sp:field-texture-S",
					"description": "Sand (field texture)"
