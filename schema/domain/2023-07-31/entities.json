							}
						},
						"roots": {
							"type": "array",
							"items": {
								"$ref": "./properties.json#/$defs/roots"
							}
						},
						"segregations": {
							"type": "array",
							"items": {
								"$ref": "./properties.json#/$defs/segregations"
							}
						},
						"pH": {
							"type": "array",
