{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "entities.json",
	"title": "ANSIS Soil Domain Entity Schema",
	"description": "JSON Schema definitions of the complete list of entities (features in GIS speak) defined in or imported by the ANSIS Domain Ontology.",
	"$comment": "Instances of entities can be created/validated using the entity-instance and entity-instance-collection schema.",
	"type": "null",
	"_curiPrefix": {
		"ansis": "https://anzsoil.org/def/au/domain/"
	},
	"$defs": {
		"Site": {
			"@id": [
				"ansis:LandsurveySite",
				"ansis:SoilSite"
			],
			"title": "ANSIS Site",
			"description": "A site where samples and observations of the landscape (ansis:LandsurveySite) and soil (ansis:SoilSite) are carried out.",
			"type": "object",
			"required": [
				"type",
				"geometry",
				"relatedProject"
			],
			"_preferred": [
				"designSite",
				"drainage",
				"landUse",
				"purpose"
			],
			"properties": {
				"id": {
					"$ref": "./base.json#/$defs/id"
				},
				"type": {
					"oneOf": [
						{
							"type": "array",
							"items": {
								"enum": [
									"ansis:LandsurveySite"
								]
							},
							"minItems": 1,
							"maxItems": 1
						},
						{
							"type": "array",
							"items": {
								"enum": [
									"ansis:LandsurveySite",
									"ansis:SoilSite"
								]
							},
							"minItems": 2,
							"maxItems": 2,
							"uniqueItems": true
						}
					]
				},
				"ansisType": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/ansisType/$defs/Site"
					}
				},
				"depthFreeWater": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/depthFreeWater"
					}
				},
				"designSite": {
					"$ref": "./properties.json#/$defs/designSite"
				},
				"disturbance": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/disturbance"
					}
				},
				"drainage": {
					"$ref": "./properties.json#/$defs/drainage"
				},
				"elevation": {
					"$ref": "./properties.json#/$defs/elevation"
				},
				"geometry": {
					"$ref": "./geo.json#/$defs/geometry"
				},
				"heightDrainage": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/heightDrainage"
					}
				},
				"landCover": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/landCover"
					}
				},
				"landManagement": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/landManagement"
					}
				},
				"landUse": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/landUse"
					}
				},
				"locationInElement": {
					"$ref": "./properties.json#/$defs/locationInElement"
				},
				"purpose": {
					"$ref": "./properties.json#/$defs/purpose"
				},
				"relatedProject": {
					"$ref": "./properties.json#/$defs/relatedProject"
				},
				"relatedSite": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/relatedSite"
					}
				},
				"scopedIdentifier": {
					"type": "array",
					"items": {
						"$ref": "./base.json#/$defs/scopedIdentifier"
					}
				},
				"siteVisit": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/siteVisit"
					}
				}
			},
			"additionalProperties": false
		},
		"SiteVisit": {
			"@id": "ansis:SiteVisit",
			"title": "ANSIS Site Visit",
			"description": "A visit to a designated site, for the purpose of taking samples, making observations, and other activities.",
			"type": "object",
			"required": [
				"type",
				"endedAtTime",
				"relatedProject",
				"startedAtTime",
				"visitor"
			],
			"_preferred": [
			],
			"properties": {
				"id": {
					"$ref": "./base.json#/$defs/id"
				},
				"type": {
					"$ref": "./base.json#/$defs/type",
