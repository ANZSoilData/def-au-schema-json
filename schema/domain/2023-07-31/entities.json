{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "entities.json",
	"title": "ANSIS Soil Domain Entity Schema",
	"description": "JSON Schema definitions of the complete list of entities (features in GIS speak) defined in or imported by the ANSIS Domain Ontology.",
	"$comment": "Instances of entities can be created/validated using the entity-instance and entity-instance-collection schema.",
	"type": "null",
	"$defs": {
		"curiPrefix": {
			"ansis": "https://anzsoil.org/def/au/domain/"
		},
		"Site": {
			"@id": [
				"ansis:LandsurveySite",
				"ansis:SoilSite"
			],
			"title": "ANSIS Site",
			"description": "A site where samples and observations of the landscape (ansis:LandsurveySite) and soil (ansis:SoilSite) are carried out.",
			"type": "object",
			"required": [
				"type",
				"geometry",
				"relatedProject"
			],
			"ansisPreferred": [
				"designSite",
				"drainage",
				"landUse",
				"purpose"
			],
			"properties": {
				"id": {
					"$ref": "./base.json#/$defs/id"
				},
				"type": {
					"oneOf": [
						{
							"type": "array",
							"items": {
								"enum": [
									"ansis:LandsurveySite"
								]
							},
							"minItems": 1,
							"maxItems": 1
						},
						{
							"type": "array",
							"items": {
								"enum": [
									"ansis:LandsurveySite",
									"ansis:SoilSite"
								]
							},
							"minItems": 2,
							"maxItems": 2,
							"uniqueItems": true
						}
					]
				},
				"ansisType": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/ansisType/$defs/site"
					}
				},
				"depthFreeWater": {
					"type": "array",
					"items": {
						"$ref": "./properties.json#/$defs/depthFreeWater"
					}
				},
